        android:supportsRtl="true"
        android:theme="@style/Theme.DropShippingCalc">
        <activity android:name=".email"
            android:screenOrientation="portrait" />
        <activity android:name=".ebayAliexpressIls"
            android:screenOrientation="portrait" />
        <activity android:name=".ebayAliexpress"
            android:screenOrientation="portrait" />
        <activity android:name=".youtube"
            android:screenOrientation="portrait" />
        <activity android:name=".ebayAmazonIls"
            android:screenOrientation="portrait" />
        <activity
            android:name=".premium"
            android:screenOrientation="portrait" />
        <activity
            android:name=".EbayAmazon"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ComingSoon"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-6247824013289332~6048698489" />
